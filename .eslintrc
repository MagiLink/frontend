{
	"root": true,
	"env": {
		"browser": true,
		"es2021": true
	},
	"loader": { ".js": "jsx" },
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off",
		"react/jsx-uses-react": "off"
	},
	"settings": {
		"react": {
			"version": "18.2.0"
		}
	}
}
